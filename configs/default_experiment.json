{
    "dataset": "cifar10",
    "tasks": 5,
    "classes_per_task": 2,
    "epochs": 20,
    "batch_size": 128,
    "lr": 0.001,
    "seed": 42,
    "optimizer": "adam",
    "metrics_frequency": 5,
    "dead_threshold": 0.95,
    "corr_threshold": 0.95,
    "saturation_threshold": 1e-4,
    "saturation_percentage": 0.99,
    "early_stopping_steps": 0,
    "reinit_output": false,
    "reinit_adam": false,
    "reset": false,
    "no_augment": false
}